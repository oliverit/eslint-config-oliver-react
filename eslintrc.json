{
  "extends": [
    "oliver-base",
    "plugin:react/recommended"
  ],
  "plugins": [
    "react"
  ],
  "parser": "babel-eslint",
  "env": {
    "browser": "true"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "rules": {
    "import/no-extraneous-dependencies": "off",
    "no-console": "off"
  }
}
